<tr id="<%= dom_id(bill) %>">
  <td class="py-4 pr-3 pl-4 text-sm font-medium whitespace-nowrap text-gray-900 sm:pl-6 dark:text-white">
    <time datetime="<%= bill.created_at.iso8601 %>"><%= l(bill.created_at.to_date, format: :long) %></time>
  </td>
  <td class="px-3 py-4 text-sm whitespace-nowrap text-gray-500 dark:text-gray-400">Draft</td>
  <td class="py-4 pr-4 pl-3 text-right text-sm font-medium whitespace-nowrap sm:pr-6">
    <%= link_to "View", bill_packages_path(bill), class: "text-indigo-600 hover:text-indigo-900 dark:text-indigo-400 dark:hover:text-indigo-300" %>
  </td>
</tr>
